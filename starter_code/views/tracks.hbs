<div class="container">
    <div class="row header-row">
        <span class="col-lg-8">Title</span>
        <span class="col-lg-4">Listen</span>
    </div>
    {{#each items}}
    <div class="row track-item">
        <div class="col-lg-8">
            <p>{{this.name}}</p>
        </div>
        <div class="col-lg-4">
            <audio controls="" name="media">
                <source src="{{this.preview_url}}">
            </audio>
        </div>
    </div>
    {{/each}}
</div>
